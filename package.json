    "scripts": {
        "test:basic": "webdriver-manager update && protractor basic/src/test/e2e/conf.js",
        "test:resource-server": "webdriver-manager update && protractor resource-server/src/test/e2e/conf.js",
        "test": "npm run test:basic",
        "basic-server": "mvn -f basic/pom.xml -Dokta.oauth2.issuer=$ORG_URL/oauth2/default -Dokta.oauth2.clientId=$CLIENT_ID -Dokta.oauth2.clientSecret=$CLIENT_SECRET",
        "resource-server": "mvn -f resource-server/pom.xml -Dokta.oauth2.issuer=$ORG_URL/oauth2/default",
        "front-end-server": "mvn -f front-end/pom.xml"
    },
    "devDependencies": {
        "chalk": "1.1.3",
        "cli-table2": "0.2.0",
        "connect": "3.5.0",
        "diff": "3.0.1",
        "forever-monitor": "1.7.1",
        "jasmine-reporters": "2.2.0",
        "jws": "3.1.4",
        "minimist": "1.2.0",
        "protractor": "5.2.0",
        "wait-on": "2.0.2"
    },
    "dependencies": {
        "body-parser": "^1.18.2",
        "chai": "3.5.0",
        "chai-files": "1.4.0",
